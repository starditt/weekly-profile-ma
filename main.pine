// ==== MA Function ====
ma_func(_src, _len, _type) =>
    switch _type
        "SMA"        => ta.sma(_src,  _len)
        "EMA"        => ta.ema(_src,  _len)
        "SMMA (RMA)" => ta.rma(_src,  _len)
        "WMA"        => ta.wma(_src,  _len)
        "VWMA"       => ta.vwma(_src, _len)
        => na

groupMA = "MA Ribbon Settings"

// ==== Global Toggle ====
show_MA_Ribbon = input.bool(true, "Show MA Ribbon", group = groupMA)

// ==== MA 1 ====
show_ma1   = input.bool(true,  "MA #1", inline = "MA1", group = groupMA)
ma1_type   = input.string("SMA", "",   inline = "MA1", group = groupMA, options = ["SMA","EMA","SMMA (RMA)","WMA","VWMA"])
ma1_len    = input.int(20,      "",    inline = "MA1", group = groupMA, minval = 1)
ma1_color  = input.color(#f6c309, "",  inline = "MA1", group = groupMA)

// ==== MA 2 ====
show_ma2   = input.bool(true,  "MA #2", inline = "MA2", group = groupMA)
ma2_type   = input.string("SMA", "",   inline = "MA2", group = groupMA, options = ["SMA","EMA","SMMA (RMA)","WMA","VWMA"])
ma2_len    = input.int(50,      "",    inline = "MA2", group = groupMA, minval = 1)
ma2_color  = input.color(#fb9800, "",  inline = "MA2", group = groupMA)

// ==== MA 3 ====
